{
  "name": "atlas-shrugged-back",
  "version": "1.0.0",
  "description": "quotation api rest express typeorm orm ",
  "main": "index.js",
  "scripts": {
    "########################################## SERVER ##############################################": "",
    "start": "node dist/index.js",
    "start:ts": "NODE_ENV=prod ts-node -r tsconfig-paths/register --transpile-only src/index.ts",
    "dev": "npx ts-node-dev -r tsconfig-paths/register --transpile-only src/index.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files ",
    "########################################## DOCKER ##############################################": "",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "########################################## TYPEORM #############################################": "",
    "create:migrate": "NODE_ENV=dev typeorm migration:create -n",
    "migrate:local": "NODE_ENV=dev ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run",
    "revert:local": "NODE_ENV=dev ts-node --transpile-only ./node_modules/typeorm/cli.js migration:revert",
    "migrate": "typeorm migration:run",
    "revert": "typeorm migration:revert",
    "########################################## FORMATTER ###########################################": "",
    "eslint": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "typeorm",
    "orm"
  ],
  "author": "italojosedev@gmail.com",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^3.1.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "excel4node": "^1.8.2",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.1",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "pg": "^8.11.2",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.7.2",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.17",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.9",
    "@babel/core": "^7.22.9",
    "@babel/node": "^7.22.6",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.22.7",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-typescript": "^7.22.5",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.8",
    "@types/nodemailer": "^6.4.13",
    "@types/socket.io": "^3.0.2",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/parser": "^6.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.46.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.1",
    "ts-node": "^10.2.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.1.6"
  }
}
